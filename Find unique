def count_unique_numbers(input_list):
    unique_numbers = []
    count = 0

    for num in input_list:
        if num not in unique_numbers:
            unique_numbers.append(num)
            count += 1

    print("Number of unique numbers:", count)
    print("Unique numbers:", unique_numbers)

# Example list
numbers = [1, 2, 3, 4, 2, 3, 5, 6, 1, 7, 8, 9, 7, 5]

# Count unique numbers and print them
count_unique_numbers(numbers)
